<?xml version="1.0" encoding="ASCII"?>
<fw-core:ApplicationModule
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:fw-core="http://www.asup.org/fw/core"
    name="DBCoreDB2">
  <services
      xsi:type="fw-core:ServicePluginRegistry"
      className="org.asup.db.core.base.BaseConnectionFactoryRegistryImpl"
      interfaceName="org.asup.db.core.QConnectionFactoryRegistry"
      pluginInterface="org.asup.db.core.QConnectionFactory">
    <services
        name="DB2"
        text="IBM DB2"
        version="0.5.0"
        vendor="www.smeup.com"
        className="org.asup.db.core.db2.DB2ConnectionFactoryImpl"/>
  </services>
  <services
      xsi:type="fw-core:ServicePluginRegistry"
      className="org.asup.db.syntax.base.BaseSyntaxBuilderRegistryImpl"
      interfaceName="org.asup.db.syntax.QSyntaxBuilderRegistry"
      pluginInterface="org.asup.db.syntax.QSyntaxBuilder">
    <services
        name="DB2"
        text="IBM DB2"
        version="0.5.0"
        vendor="www.smeup.com"
        className="org.asup.db.syntax.db2.DB2SyntaxBuilderImpl"/>
  </services>
  <services
      xsi:type="fw-core:ServicePluginRegistry"
      className="org.asup.db.syntax.base.BaseQueryConverterRegistryImpl"
      interfaceName="org.asup.db.syntax.QQueryConverterRegistry"
      pluginInterface="org.asup.db.syntax.QQueryConverter">
    <services
        name="DB2"
        text="IBM DB2"
        version="0.5.0"
        vendor="www.smeup.com"
        className="org.asup.db.syntax.db2.DB2QueryConverterImpl"/>
  </services>
</fw-core:ApplicationModule>
